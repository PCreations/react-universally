    "isomorphic-fetch": "^2.2.1",
    "loader-utils": "^0.2.16",
    "modux-js": "0.0.6",
    "normalize.css": "5.0.0",
    "path": "0.12.7",
    "react": "15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-apollo": "^0.5.13",
    "react-dom": "15.3.2",
